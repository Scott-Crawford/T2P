{
  "manifest_version": 2,

  "name": "T2P",
  "description": "This extension replaces no-no words with pictures",
  "version": "1.0",
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Settings"
  },
  
  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-2.2.0.min.js", "myscript.js"]
    }
  ]


  
}